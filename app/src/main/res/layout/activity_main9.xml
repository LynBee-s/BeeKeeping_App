        <EditText
            android:id="@+id/amtt9"
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:inputType="textPersonName"
            android:fontFamily="@font/alice"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
